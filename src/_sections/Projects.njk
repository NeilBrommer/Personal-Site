---
title: Projects
eleventyNavigation:
  key: Projects
  icon: tool
  order: 1
tags: [ "MainPage" ]
sectionOrder: 1
---

<div class="row-lg-2">
	{% for project in projects %}
		<section class="col card">
			<h3>{{ project.name }}</h3>
			<p>{{ project.description }}</p>
			<ul class="card-links">
				{% for link in project.links %}
					<li>
						<a href="{{ link.url }}">{{ link.title }}</a>
					</li>
				{% endfor %}
			</ul>
		</section>
	{% endfor %}
</div>
